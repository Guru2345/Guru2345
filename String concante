class StringConcatenation {
    public static void main(String[] args) {
        String str1 = "asifa";
        String str2 = "vardhan";
        StringBuilder sb = new StringBuilder();
        sb.append(str1);
        sb.append(" ");
        sb.append(str2);
        String result = sb.toString();
        System.out.println(result);  
    }
}
