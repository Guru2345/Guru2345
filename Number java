import java.util.*;

class Numbertri {
    public static void main(String args[]) {
        int n = 5;    
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j<= n-i ; j++) {
                System.out.print(i);
            }
          System.out.println("");
        }
    }
}
1111
222
33
4
